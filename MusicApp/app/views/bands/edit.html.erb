<h1>Editing Band</h1>

<form action= <%= band_url(@band) %> method = 'post'>
  <input type="hidden" name="_method" value="put">
  <%= auth_token %>
  <label for="name">Band Name</label>

  <input type="text" name ="band[name]" id="name", value= <%= @band.name %>>

  <input type="submit" value="Edit band">
</form>
<%= link_to 'Back', bands_path %>
