<p id="notice"><%= notice %></p>

<h1>Listing Albums</h1>
<%= auth_token %>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @albums.each do |album| %>
      <%= link_to "#{album.name}", album_url(album) %><br>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Album', new_album_path %>
<%= link_to 'Bands', bands_url %>
<%= link_to 'Tracks', tracks_url %>
